  width: 1fr;
  }



.slider {
  display: grid;
  width: 5%;
  -webkit-appearance: slider-vertical;
}


.wood_points, .fire_points, .earth_points, .metal_points, .water_points {
  width: 1fr;
}

.wood_bar_yin, .wood_bar_yang, .fire_bar_yin, .fire_bar_yang, .earth_bar_yin, .earth_bar_yang,
.metal_bar_yin, .metal_bar_yang, .water_bar_yin, .water_bar_yang {
