.theme-light {
  filter: invert(0%);
   background-color:    #dfddb9;
}

.theme-dark {
  filter: invert(10%);
  background-color:  darkolivegreen;
  color: orange;
}

.theme-olive {
  background-color:  rgba(128, 128, 0, 0.7);
  color: black;
}

.page, .prev, .next, .last, .first {
  display: inline-grid;
  min-width: 8vmin;
  max-width: 12vmin;
  text-align: center;
}

.page.current {
  background-color: yellow;
  border: 2px solid red;
  border-radius: 10px;
}

.theme-grey {
  filter: invert(10%);
  background-color: grey;
  color: black;
}
.canvas-style {
    border: 5px solid red;
}

.canvas-style-2 {
  border: 10px solid blue;
}

.reversed_list {
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
}

.reversed_list>li {
  margin-right: 20px;
  margin-bottom: 25px;
  -webkit-transform: rotate(-180deg);
  transform: rotate(-180deg);
}

.reversed_list_without_bullets {
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
  margin-bottom: 0px;
}

.reversed_list_without_bullets>li {
  list-style: none;
}

.histogramme {
  display: grid;
  grid-template-columns: repeat(12, 1fr);
  grid-template-rows: 25vmax 400px;

}


 input[type=range]::-webkit-slider-thumb {
  -webkit-appearance: none;
  width: 10px;
  height: 35px;
  border: 3px solid #000000;

  background: orange;
  cursor: pointer;
  box-shadow: -2px 2px 2px #000000, 0px 0px 1px #0d0d0d;
}


.indicator {
  max-width: 100%;
  min-width: 100%;
  box-shadow: 5px 2px 8px 0px #595527;
  height: 50%;
  max-height: 100%;
  transition: height 0.3s cubic-bezier(0, .5, .5, 1);
  border-top: 3px solid black;
  border-left: 1px solid black;
  border-right: 1px solid black;
  border-radius: 2px;
}

.indicator_wrapper {
  display: grid;
  border-top: 1px solid grey;
  border-bottom: 1px solid grey;
  height: 80%;
  justify-items: center;
  align-items: end;
  background: rgb(208,204,149);
  background: linear-gradient(0deg, rgba(208,204,149,1) 50%, rgba(153,0,204,1) 50%, rgba(223,221,185,1) 51%);
}

#wood_yin_range, #wood_yang_range, #fire_yin_range, #fire_yang_range,
 #fire_in_earth_yin_range, #fire_in_earth_yang_range, #earth_yin_range,
 #earth_yang_range, #metal_yin_range, #metal_yang_range, #water_yin_range,
 #water_yang_range {
  display: grid;
  height: 100%;
  width: 96%;
  justify-items: center;
  align-items: end;
}

