.indicator_background {
  display: grid;
  opacity: 95%;
  max-width: 25%;
  min-width: 25%;
  height: 100%;
  border-top: 1px solid grey;
  border-left: 1px solid grey;
  border-right: 1px solid grey;
  border-radius: 2px;
  box-shadow: 5px 2px 8px 0px #8e883e;
